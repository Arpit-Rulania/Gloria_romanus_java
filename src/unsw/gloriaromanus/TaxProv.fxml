<VBox maxWidth="403.0" prefHeight="490.0" prefWidth="219.0" spacing="10" styleClass="panel-region" stylesheets="/unsw/gloriaromanus/style.css" StackPane.alignment="TOP_LEFT" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unsw.gloriaromanus.TaxProvController">
    <padding>
        <Insets topRightBottomLeft="10" />
    </padding>
    <background>
        <Background>
            <fills>
                <BackgroundFill>
                    <fill>
                        <Color opacity="0.3" />
                    </fill>
                </BackgroundFill>
            </fills>
        </Background>
    </background>
   <Label prefHeight="40.0" prefWidth="181.0" text="          SET TAX" textAlignment="CENTER" textFill="#bd1ddd">
      <font>
         <Font name="Chalkboard SE Light" size="16.0" />
      </font>
   </Label>
   <VBox prefHeight="142.0" prefWidth="383.0">
      <children>
         <VBox prefHeight="90.0" prefWidth="199.0">
            <children>
               <Label text="                      Player 1">
                  <font>
                     <Font size="15.0" />
                  </font>
               </Label>
               <Label text="           " />
               <ComboBox fx:id="getTax1" onAction="#setTax1" prefHeight="27.0" prefWidth="195.0" promptText="choose tax level" />
               <Label text="                                      OR" />
               <HBox prefHeight="27.0" prefWidth="185.0">
                  <children>
                     <Button mnemonicParsing="false" onAction="#inc5p1" prefHeight="27.0" prefWidth="101.0" text="increase 5%" />
                     <Button mnemonicParsing="false" onAction="#dec5p1" prefHeight="27.0" prefWidth="91.0" text="decrease 5%" />
                  </children>
               </HBox>
            </children>
         </VBox>
         <Label text="                                               " />
         <Label text="                                               " />
         <Label text="                                               " />
      </children>
   </VBox>
   <HBox prefHeight="100.0" prefWidth="169.0">
      <children>
         <VBox prefHeight="49.0" prefWidth="191.0">
            <children>
               <Label text="                       Player 2">
                  <font>
                     <Font size="15.0" />
                  </font>
               </Label>
               <Label text="           " />
               <ComboBox fx:id="getTax2" onAction="#setTax2" prefHeight="27.0" prefWidth="192.0" promptText="choose tax level" />
               <Label text="                                      OR" />
               <HBox prefHeight="100.0" prefWidth="200.0">
                  <children>
                     <Button mnemonicParsing="false" onAction="#inc5p2" prefHeight="27.0" prefWidth="95.0" text="increase 5%" />
                     <Button mnemonicParsing="false" onAction="#dec5p2" prefHeight="27.0" prefWidth="99.0" text="decrease 5%" />
                  </children>
               </HBox>
            </children>
         </VBox>
      </children>
   </HBox>
   <TextArea fx:id="setprovtaxArea" prefHeight="97.0" prefWidth="199.0" />
    <Button onAction="#clickedSwitchMenufromTaxProv" text="Switch Menus" />
</VBox>
