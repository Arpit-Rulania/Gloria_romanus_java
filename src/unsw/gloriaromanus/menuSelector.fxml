<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Background?>
<?import javafx.scene.layout.BackgroundFill?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.paint.Color?>
<?import javafx.scene.text.Font?>

<VBox maxWidth="250" spacing="10" styleClass="panel-region" stylesheets="/unsw/gloriaromanus/style.css" StackPane.alignment="TOP_LEFT" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="unsw.gloriaromanus.menuSelectorController">
    <padding>
        <Insets topRightBottomLeft="10" />
    </padding>
    <background>
        <Background>
            <fills>
                <BackgroundFill>
                    <fill>
                        <Color opacity="0.3" />
                    </fill>
                </BackgroundFill>
            </fills>
        </Background>
    </background>
   <Button mnemonicParsing="false" onAction="#attackmenucalledb" prefHeight="203.5" prefWidth="230.0" text="ATTACK!!!" textFill="#9d40b8">
      <font>
         <Font name="Rockwell" size="32.0" />
      </font>
   </Button>
   <Button mnemonicParsing="false" onAction="#buildmenucalledb" prefHeight="203.5" prefWidth="230.0" text="SHOP" textFill="#9d40b8">
      <font>
         <Font name="Rockwell" size="32.0" />
      </font>
   </Button>
   <Button mnemonicParsing="false" onAction="#taxretebuttionb" prefHeight="203.5" prefWidth="230.0" text="TAX OFFICE" textFill="#9d40b8">
      <font>
         <Font name="Rockwell" size="27.0" />
      </font>
   </Button>
   <Button mnemonicParsing="false" onAction="#menumenunecxtturn" prefHeight="203.5" prefWidth="230.0" text="NEXT TURN" textFill="#9d40b8">
      <font>
         <Font name="Rockwell" size="27.0" />
      </font>
   </Button>
</VBox>
